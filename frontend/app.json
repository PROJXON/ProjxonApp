{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "We use your camera to capture photos and videos to attach to messages.",
        "NSMicrophoneUsageDescription": "We use your microphone when recording video attachments.",
        "NSPhotoLibraryUsageDescription": "We use your photo library to pick media to attach to messages."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.frontend",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "WS_URL": "wss://a6ersp1gab.execute-api.us-east-2.amazonaws.com/production",
      "API_URL": "https://828bp5ailc.execute-api.us-east-2.amazonaws.com"
    },
    "plugins": [
      "expo-secure-store",
      "expo-camera",
      "expo-video",
      "expo-notifications"
    ]
  }
}
